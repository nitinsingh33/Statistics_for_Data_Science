    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from math import comb, factorial\n",
    "\n",
    "def binomial_dis(n,p)\n",
    "    x=np.arange(0,n+1)\n",
    "    pmf=[comb(n,k)*(p**k)*((1-p)**(n-k)) for k in x]\n",
    "    cdf=np.cumsum(pmf)\n",
    "    exp_x=np.sum([k*pmf[k] for k in x])\n",
    "    exp_xx=np.sum([(k**2)*pmf[k]])"
   ]
  }
